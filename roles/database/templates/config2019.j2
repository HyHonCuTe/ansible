[OPTIONS]
ACTION="Install"
ENU="True"
FEATURES=SQLENGINE
INSTANCENAME=MSSQLSERVER
SQLSVCACCOUNT="NT Service\MSSQLSERVER"  # Dùng tài khoản hệ thống cho SQL Server service
SQLSVCPASSWORD="YourPasswordHere"  # Nếu không cần mật khẩu, có thể bỏ dòng này hoặc để trống
AGTSVCACCOUNT="NT Service\SQLSERVERAGENT"
SQLSYSADMINACCOUNTS="DOMAIN\YourAdminAccount"
SECURITYMODE="Windows"  # Sử dụng Windows Authentication thay vì SQL Authentication
